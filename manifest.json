{
    "manifest_version": 2,
    "name": "Edge Internal Pages Extension",
    "description": "Automate Edge internal page actions.",
    "version": "1.0",
    "permissions": [
      "clipboardWrite",
      "activeTab",
      "tabs"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  